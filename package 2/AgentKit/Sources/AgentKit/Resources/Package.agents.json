{
  "agents": [
    {
      "id": "agent.orchestrator",
      "name": "Agent Orchestrator",
      "description": "Coordinates multiple agents and manages task distribution",
      "triggers": [
        { "type": "event", "condition": "task.created" },
        { "type": "event", "condition": "agent.idle" }
      ],
      "actions": [
        "analyze.task",
        "select.agent",
        "assign.task",
        "monitor.progress"
      ],
      "config": {
        "autoStart": true,
        "priority": "high",
        "maxConcurrent": 1
      }
    },
    {
      "id": "agent.health.monitor",
      "name": "Health Monitor",
      "description": "Monitors agent health and performance",
      "triggers": [
        { "type": "schedule", "condition": "every.minute" },
        { "type": "event", "condition": "agent.error" }
      ],
      "actions": [
        "check.health",
        "collect.metrics",
        "alert.issues",
        "restart.agent"
      ],
      "config": {
        "autoStart": true,
        "priority": "critical",
        "maxConcurrent": 1
      }
    },
    {
      "id": "agent.memory.manager",
      "name": "Memory Manager",
      "description": "Manages agent memory and context",
      "triggers": [
        { "type": "event", "condition": "memory.full" },
        { "type": "schedule", "condition": "hourly" }
      ],
      "actions": [
        "analyze.memory",
        "prune.old",
        "consolidate.context",
        "optimize.storage"
      ],
      "config": {
        "autoStart": true,
        "priority": "normal",
        "maxConcurrent": 1
      }
    },
    {
      "id": "agent.task.prioritizer",
      "name": "Task Prioritizer",
      "description": "Prioritizes tasks based on urgency and importance",
      "triggers": [
        { "type": "event", "condition": "task.created" },
        { "type": "event", "condition": "deadline.approaching" }
      ],
      "actions": [
        "analyze.tasks",
        "calculate.priority",
        "reorder.queue",
        "notify.changes"
      ],
      "config": {
        "autoStart": true,
        "priority": "normal",
        "maxConcurrent": 1
      }
    }
  ]
}
