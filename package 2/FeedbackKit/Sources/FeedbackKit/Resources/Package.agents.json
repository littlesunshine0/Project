{
  "agents": [
    {
      "id": "feedback.collector",
      "name": "Feedback Collector",
      "description": "Collects and processes feedback",
      "triggers": [
        { "type": "event", "condition": "feedback.submitted" }
      ],
      "actions": [
        "validate.feedback",
        "categorize.feedback",
        "store.feedback",
        "notify.team"
      ],
      "config": {
        "autoStart": true,
        "priority": "normal",
        "maxConcurrent": 1
      }
    },
    {
      "id": "feedback.analyzer",
      "name": "Feedback Analyzer",
      "description": "Analyzes feedback sentiment and trends",
      "triggers": [
        { "type": "schedule", "condition": "daily" }
      ],
      "actions": [
        "analyze.sentiment",
        "identify.trends",
        "generate.report"
      ],
      "config": {
        "autoStart": false,
        "priority": "low",
        "maxConcurrent": 1
      }
    }
  ]
}
