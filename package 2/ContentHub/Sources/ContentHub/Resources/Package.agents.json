{
  "agents": [
    {
      "id": "content.syncer",
      "name": "Content Syncer",
      "description": "Syncs content from sources",
      "triggers": [
        { "type": "schedule", "condition": "hourly" },
        { "type": "event", "condition": "source.added" }
      ],
      "actions": [
        "fetch.content",
        "process.content",
        "store.content"
      ],
      "config": {
        "autoStart": true,
        "priority": "normal",
        "maxConcurrent": 2
      }
    }
  ]
}
