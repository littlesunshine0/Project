{
  "menus": [
    {
      "id": "analytics.main",
      "type": "main",
      "items": [
        { "id": "analytics.dashboard", "title": "Dashboard", "icon": "chart.bar", "action": "dashboard.show" },
        { "id": "analytics.reports", "title": "Reports", "icon": "doc.text", "action": "report.list" },
        { "id": "analytics.export", "title": "Export", "icon": "square.and.arrow.up", "action": "analytics.export" }
      ]
    },
    {
      "id": "analytics.context",
      "type": "context",
      "items": [
        { "id": "ctx.details", "title": "View Details", "icon": "info.circle", "action": "metric.details" },
        { "id": "ctx.export", "title": "Export", "icon": "square.and.arrow.up", "action": "metric.export" }
      ]
    }
  ],
  "views": [
    { "id": "analytics.browser", "type": "browser", "title": "Analytics", "icon": "chart.bar" },
    { "id": "analytics.dashboard", "type": "canvas", "title": "Dashboard", "icon": "square.grid.2x2" },
    { "id": "analytics.reports", "type": "list", "title": "Reports", "icon": "doc.text" }
  ],
  "animations": [
    { "id": "chart.update", "type": "fade", "duration": 0.3, "curve": "easeInOut" },
    { "id": "metric.change", "type": "scale", "duration": 0.2, "curve": "spring" }
  ],
  "icons": [
    { "id": "analytics.main", "systemName": "chart.bar" },
    { "id": "analytics.metric", "systemName": "number" },
    { "id": "analytics.report", "systemName": "doc.text" }
  ],
  "colorScheme": "adaptive",
  "accessibility": {
    "voiceOver": true,
    "reduceMotion": true,
    "highContrast": true,
    "dynamicType": true
  }
}
