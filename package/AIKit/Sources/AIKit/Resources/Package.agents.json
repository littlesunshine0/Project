{
  "agents": [
    {
      "id": "ai.model.manager",
      "name": "Model Manager",
      "description": "Manages AI model lifecycle",
      "triggers": [
        { "type": "event", "condition": "app.launch" },
        { "type": "event", "condition": "model.request" }
      ],
      "actions": [
        "check.models",
        "load.required",
        "unload.unused",
        "optimize.memory"
      ],
      "config": {
        "autoStart": true,
        "priority": "high",
        "maxConcurrent": 1
      }
    },
    {
      "id": "ai.cache.manager",
      "name": "Cache Manager",
      "description": "Manages AI response caching",
      "triggers": [
        { "type": "event", "condition": "prediction.completed" },
        { "type": "schedule", "condition": "hourly" }
      ],
      "actions": [
        "cache.result",
        "check.cache",
        "prune.old",
        "optimize.storage"
      ],
      "config": {
        "autoStart": true,
        "priority": "low",
        "maxConcurrent": 1
      }
    },
    {
      "id": "ai.quality.monitor",
      "name": "Quality Monitor",
      "description": "Monitors AI output quality",
      "triggers": [
        { "type": "event", "condition": "generation.completed" }
      ],
      "actions": [
        "analyze.output",
        "check.quality",
        "log.metrics",
        "alert.issues"
      ],
      "config": {
        "autoStart": false,
        "priority": "low",
        "maxConcurrent": 1
      }
    }
  ]
}
