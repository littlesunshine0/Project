{
  "agents": [
    {
      "id": "search.indexer",
      "name": "Auto Indexer",
      "description": "Automatically indexes content for search",
      "triggers": [
        { "type": "fileChange", "condition": "*" },
        { "type": "schedule", "condition": "hourly" }
      ],
      "actions": [
        "detect.changes",
        "parse.content",
        "update.index",
        "optimize.index"
      ],
      "config": {
        "autoStart": true,
        "priority": "low",
        "maxConcurrent": 1
      }
    },
    {
      "id": "search.suggester",
      "name": "Search Suggester",
      "description": "Provides intelligent search suggestions",
      "triggers": [
        { "type": "event", "condition": "user.typing" }
      ],
      "actions": [
        "analyze.query",
        "find.similar",
        "rank.suggestions"
      ],
      "config": {
        "autoStart": true,
        "priority": "high",
        "maxConcurrent": 1
      }
    },
    {
      "id": "search.ranker",
      "name": "Result Ranker",
      "description": "Ranks search results by relevance",
      "triggers": [
        { "type": "event", "condition": "search.completed" }
      ],
      "actions": [
        "analyze.results",
        "calculate.relevance",
        "apply.personalization",
        "reorder.results"
      ],
      "config": {
        "autoStart": true,
        "priority": "normal",
        "maxConcurrent": 1
      }
    }
  ]
}
