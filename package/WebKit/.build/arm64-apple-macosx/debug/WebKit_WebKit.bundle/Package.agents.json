{
  "agents": [
    {
      "id": "web.cache.manager",
      "name": "Cache Manager",
      "description": "Manages web content cache",
      "triggers": [
        { "type": "event", "condition": "page.loaded" },
        { "type": "schedule", "condition": "hourly" }
      ],
      "actions": [
        "cache.content",
        "prune.old",
        "optimize.storage"
      ],
      "config": {
        "autoStart": true,
        "priority": "low",
        "maxConcurrent": 1
      }
    }
  ]
}
